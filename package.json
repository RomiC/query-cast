{
  "name": "query-cast",
  "version": "2.0.1",
  "description": "Query params parser and cast micro-library",
  "repository": {
    "type": "git",
    "url": "git@github.com:RomiC/query-cast.git"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "lint": "tslint -c tslint.json '{src,tests}/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --no-cache"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "typeable": "2.4.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "lint-staged": "12.3.3",
    "ts-jest": "26.5.6",
    "tslint": "6.1.3",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "4.6.4"
  },
  "keywords": [
    "query",
    "querystring",
    "searchparams",
    "query parameters",
    "url parser",
    "query parser"
  ],
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}
