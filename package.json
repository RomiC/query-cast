{
  "name": "query-cast",
  "version": "2.0.1",
  "description": "Query params parser and cast micro-library",
  "repository": {
    "type": "git",
    "url": "git@github.com:RomiC/query-cast.git"
  },
  "engines": {
    "node": ">=14"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "lint": "tslint -c tslint.json '{src,tests}/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest --config vitest.config.ts run"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "typeable": "2.4.1"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "0.31.3",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "tslint": "5.20.1",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "5.0.4",
    "vitest": "0.31.2"
  },
  "keywords": [
    "query",
    "querystring",
    "searchparams",
    "query parameters",
    "url parser",
    "query parser"
  ],
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}
